{
  "name": "cardgallery",
  "version": "1.0.0",
  "description": "Static card gallery site powered by Cloudinary and GitHub Pages.",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
