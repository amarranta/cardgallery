    "cloudinary": "^1.39.2",
    "leaflet": "^1.9.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/tailwind": "^5.1.0",
    "astro": "^4.14.0",
    "autoprefixer": "^10.4.16",
    "dotenv": "^17.2.3",
    "@types/leaflet": "^1.9.21",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.9.3"
  }
